{
    "name":"shopwarelabs/swag-media-azure",
    "type": "shopware-plugin",
    "description": "Shopware media adapter to store media files on Azure Blob Storage.",
    "comment": "league/flysystem and guzzlehttp/guzzle are provided by shopware",
    "keywords": ["shopware", "azure"],
    "config": {
    	"optimize-autoloader": true
	},
    "archive": {
    	"exclude": ["/shopwarelabs-swag-media-azure-*.*", "/.git", "/.gitignore", "!/vendor"]
    },
    "require": {
        "league/flysystem-azure-blob-storage": "0.1.4",
        "guzzlehttp/psr7":"2.2.1",
        "guzzlehttp/guzzle":"7.4.1"
    },
    "replace": {

    }
}
